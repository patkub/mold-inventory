{
  "name": "mold-inventory-app",
  "version": "1.10.0",
  "private": true,
  "scripts": {
    "dev": "node wrangler-to-env.js dev && next dev --turbopack",
    "dev:staging": "node wrangler-to-env.js staging && next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc --noEmit",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "test": "node wrangler-to-env.js dev && vitest run",
    "test:watch": "node wrangler-to-env.js dev && vitest",
    "cf:build": "node wrangler-to-env.js dev && opennextjs-cloudflare build --env dev",
    "preview": "node wrangler-to-env.js dev && opennextjs-cloudflare build && opennextjs-cloudflare preview --env dev",
    "preview:staging": "node wrangler-to-env.js staging && opennextjs-cloudflare build && opennextjs-cloudflare preview --env staging",
    "deploy": "node wrangler-to-env.js production && opennextjs-cloudflare build && opennextjs-cloudflare deploy --env production",
    "upload": "node wrangler-to-env.js production && opennextjs-cloudflare build && opennextjs-cloudflare upload --env production",
    "cf-typegen": "wrangler types"
  },
  "dependencies": {
    "@auth0/auth0-react": "^2.6.0",
    "@opennextjs/cloudflare": "^1.9.1",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.544.0",
    "next": "15.5.4",
    "next-themes": "^0.4.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tw-animate-css": "^1.4.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@tailwindcss/postcss": "^4.1.14",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.17.0",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "@vitejs/plugin-react": "^5.0.4",
    "eslint": "^9.37.0",
    "eslint-config-next": "15.5.4",
    "eslint-config-prettier": "^10.1.8",
    "jsdom": "^27.0.0",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.14",
    "typescript": "^5.9.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "~3.2.4",
    "vitest-mock-extended": "^3.1.0",
    "wrangler": "^4.42.0"
  }
}
