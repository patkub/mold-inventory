// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
  output   = "../.generated/prisma"
}
datasource db {
  provider = "sqlite"
}

model molds {
  id    Int     @id @default(autoincrement())
  number String  @unique
  description String?
  cycle_time Int
  status String?
}
